{
  "name": "Google Form Viewer PWA",
  "id": "com.googleformviewer",
  "short_name": "FormViewer",
  "description": "A PWA wrapper for displaying Google Forms.",
  "start_url": "index.html",
  "display": "standalone",
  "background_color": "#23482F",
  "theme_color": "#2A5A3A",
  "launch_handler": {
    "client_mode": "navigate-new"
  },
  "icons": [
    {
      "src": "icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
